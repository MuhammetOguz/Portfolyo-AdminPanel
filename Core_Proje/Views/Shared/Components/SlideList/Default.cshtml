@model List<Portfolio>
<div class="col-md-6 col-xl-4 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Projelerimden Görüntüler</h4>
            <div class="owl-carousel owl-theme full-width owl-carousel-dash portfolio-carousel" id="owl-carousel-basic">
                @foreach (var item in Model)
                {
                    <div class="item">
                        <img src="@item.ImageUrl" alt="" widht="300" height="200">
                    </div>
                }       
                </div>
             
            <div class="d-flex py-4">
                <div class="preview-list w-100">
                    <div class="preview-item p-0">
                        <div class="preview-thumbnail">
                            <img src="~/corona-free-dark-bootstrap-admin-template-1.0.0/Template/assets/images/faces/moguz.jpg" class="rounded-circle" alt="">
                        </div>
                        <div class="preview-item-content d-flex flex-grow">
                            <div class="flex-grow">
                                <div class="d-flex d-md-block d-xl-flex justify-content-between">
                                    <h6 class="preview-subject">Muhammet Oğuz</h6>
                                    <p class="text-muted text-small"></p>
                                </div>
                                <p class="text-muted">.NET Developer</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <p class="text-muted">Projelerime ait görüntüler </p>
            <div class="progress progress-md portfolio-progress">
                <div class="progress-bar bg-success" role="progressbar" style="width: 90%" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
        </div>
    </div>
</div>